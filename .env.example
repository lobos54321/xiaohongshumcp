# 环境配置
APP_ENV=development
APP_PORT=8080
APP_HOST=localhost

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=password
DB_NAME=xiaohongshu_proxy
DB_SSL_MODE=disable

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# JWT 配置
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# Claude API 配置
CLAUDE_API_KEY=your-claude-api-key
CLAUDE_BASE_URL=https://api.anthropic.com

# MCP 配置
MCP_BASE_URL=http://localhost:18060/mcp
MCP_TIMEOUT=30s

# 容器配置
DOCKER_REGISTRY=xiaohongshu-proxy
CONTAINER_BASE_PORT=19000
MAX_CONTAINERS_PER_USER=3

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json